<%= form_for @project do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, class:"form-control" %><br>
  <%= f.label :description %>
  <%= f.text_field :description, class:"form-control" %><br>
  <%= f.label :due_date %>
  <%= f.date_field :due_date, class:"form-control" %><br>
  <%= f.label :memberships_id %><br>
  <%= f.collection_check_boxes :user_ids, User.all, :id, :full_name %><br>
  <%= f.label :tags %><br>
  <%= f.collection_check_boxes :tag_ids, Tag.order("title"), :id, :title %>
  <%= f.submit class: "btn btn-info btn-md" %>
<% end %>
